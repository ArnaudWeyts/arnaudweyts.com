    script.
      var elements = document.getElementsByClassName('stroke');
      for (var i = elements.length - 1; i >= 0; i--) {
        console.log('adding new element');
        var timer = setInterval(function(elem) {
          if(parseInt(elem.getAttribute("stroke-dashoffset")) == 1000 - parseInt(elem.getAttribute("data-amount"))) {
            clearInterval(timer);
            console.log('interval cleared');
          }
          stroke(elem);
        }, 5, elements[i]);
      };
      function stroke(elem) {
        elem.setAttribute("stroke-dashoffset", parseInt(elem.getAttribute("stroke-dashoffset"))-1);
      }





